{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@packages/*": [
        "../../packages/*"
      ]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
  ],
  "exclude": [
    "node_modules",
  ]
}
