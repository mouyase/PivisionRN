{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    // JavaScript 支持：已启用
    "allowJs": true,
    // 无法访问的代码检查：已启用
    "allowUnreachableCode": false,
    // 未使用标签检查：已启用
    "allowUnusedLabels": false,
    // CommonJS 和 ES 模块互操作性：已启用
    "esModuleInterop": true,
    // 文件名大小写一致性检查：已启用
    "forceConsistentCasingInFileNames": true,
    // JSX 支持类型
    "jsx": "react-native",
    // 标准库
    "lib": [
      "ESNext"
    ],
    // 模块系统
    "module": "ESNext",
    // 模块解析策略
    "moduleResolution": "node",
    // switch 语句中的 fallthrough 检查：已启用
    "noFallthroughCasesInSwitch": true,
    // 输出文件生成：已禁止
    "noEmit": true,
    // 函数返回值检查：已启用
    "noImplicitReturns": true,
    // 隐式 'use strict' 禁用：已禁止
    "noImplicitUseStrict": false,
    // 泛型检查禁用：已禁止
    "noStrictGenericChecks": false,
    // 索引签名结果添加 undefined：已启用
    "noUncheckedIndexedAccess": true,
    // 未使用局部变量检查：已启用
    "noUnusedLocals": true,
    // 未使用参数检查：已启用
    "noUnusedParameters": true,
    // package.json 导入解析：已禁止
    "resolvePackageJsonImports": false,
    // JSON 模块导入：已启用
    "resolveJsonModule": true,
    // 类型声明文件的类型检查跳过：已启用
    "skipLibCheck": true,
    // 所有严格类型检查选项：已启用
    "strict": true,
    // ECMAScript 目标版本
    "target": "ESNext",
    // 精确模块语法保留：已启用
    "verbatimModuleSyntax": true,
  },
}
